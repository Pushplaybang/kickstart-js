{
  "name": "kickstart-js",
  "version": "1.0.0",
  "description": "minmal starter for javascript based projects",
  "main": "index.js",
  "repository": "https://github.com/Pushplaybang/kickstart-js",
  "author": "pushplaybang <pushplaybang@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest ./tests",
    "test:watch": "jest ./tests --watchAll --coverage --notify",
    "start": "echo \"No start command defined, set this up for your project\"",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "lint": "eslint \"src/**\"",
    "serve:coverage": "http-server ./coverage/lcov-report -p 7357",
    "update:packages": "rm -rf node_modules && rm package-lock.json && npm install && npm update --all && npm outdated"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@sigmadigital/configs": "0.0.14",
    "autoprefixer": "^9.4.7",
    "babel-loader": ">= 8",
    "clean-webpack-plugin": ">= 1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": ">= 2",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": ">= 5",
    "flow-bin": ">= 0.91",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": ">= 24",
    "node-sass": ">= 4",
    "postcss-scss": ">= 2",
    "prettier": ">= 1.16.1",
    "sass-loader": "^7.1.0",
    "style-loader": ">= 0.23",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
